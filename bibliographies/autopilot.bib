@article{engineerEnvironmentalEnrichmentImproves2004a,
  title = {Environmental {{Enrichment Improves Response Strength}}, {{Threshold}}, {{Selectivity}}, and {{Latency}} of {{Auditory Cortex Neurons}}},
  author = {Engineer, Navzer D. and Percaccio, Cherie R. and Pandya, Pritesh K. and Moucha, Raluca and Rathbun, Daniel L. and Kilgard, Michael P.},
  year = {2004},
  month = jul,
  journal = {Journal of Neurophysiology},
