@misc{ponziSciencePyramidScheme2020,
  title = {Is Science a Pyramid Scheme? {{The}} Correlation between an Author's Position in the Academic Hierarchy and Her Scientific Output per Year},
  shorttitle = {Is Science a Pyramid Scheme?},
  author = {Ponzi, Charleeze},
