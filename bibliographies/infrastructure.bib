@article{mortonROBOKOPAbstractionLayer2019,
  title = {{{ROBOKOP}}: An Abstraction Layer and User Interface for Knowledge Graphs to Support Question Answering},
  shorttitle = {{{ROBOKOP}}},
  author = {Morton, Kenneth and Wang, Patrick and Bizon, Chris and Cox, Steven and Balhoff, James and Kebede, Yaphet and Fecho, Karamarie and Tropsha, Alexander},
  year = {2019},
  month = dec,
  journal = {Bioinformatics},
