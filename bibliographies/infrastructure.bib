@article{ramTransphobiaEncodedExamination2021,
  title = {Transphobia, Encoded: An Examination of Trans-Specific Terminology in {{SNOMED CT}} and {{ICD-10-CM}}},
  shorttitle = {Transphobia, Encoded},
  author = {Ram, A and Kronk, Clair A and Eleazer, Jacob R and Goulet, Joseph L and Brandt, Cynthia A and Wang, Karen H},
  year = {2021},
  month = sep,
  journal = {Journal of the American Medical Informatics Association},
  number = {ocab200},
