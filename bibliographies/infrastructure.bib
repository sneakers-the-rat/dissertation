  author = {Korn, Daniel and Thieme, Andrew J. and Alves, Vinicius M. and Yeakey, Michael and Borba, Joyce V. V. B. and Capuzzi, Stephen J. and Fecho, Karamarie and Bizon, Chris and Edwards, Stephen W. and Chirkova, Rada and Colvis, Christine M. and Southall, Noel T. and Austin, Christopher P. and Muratov, Eugene N. and Tropsha, Alexander},
  year = {2022},
  month = jun,
  journal = {Drug Discovery Today},
  volume = {27},
  number = {6},
  pages = {1671--1678},
  issn = {1359-6446},
  doi = {10.1016/j.drudis.2022.02.008},
  url = {https://www.sciencedirect.com/science/article/pii/S1359644622000654},
  urldate = {2022-06-18},
  abstract = {Here, we propose a broad concept of `Clinical Outcome Pathways' (COPs), which are defined as a series of key molecular and cellular events that underlie therapeutic effects of drug molecules. We formalize COPs as a chain of the following events: molecular initiating event (MIE)~\textrightarrow ~intermediate event(s)~\textrightarrow ~clinical outcome. We illustrate the concept with COP examples both for primary and alternative (i.e., drug repurposing) therapeutic applications. We also describe the elucidation of COPs for several drugs of interest using the publicly accessible Reasoning Over Biomedical Objects linked in Knowledge-Oriented Pathways (ROBOKOP) biomedical knowledge graph-mining tool. We propose that broader use of COP uncovered with the help of biomedical knowledge graph mining will likely accelerate drug discovery and repurposing efforts.},
  langid = {english},
  keywords = {Clinical outcome pathways,Drug repurposing,Key molecular events,Knowledge graphs},
  file = {/Users/jonny/Dropbox/papers/zotero/K/KornD/korn_2022_defining_clinical_outcome_pathways.pdf}
}

@inproceedings{kornEInfrastructuresResearchCollaboration2017,
  title = {E-{{Infrastructures}} for {{Research Collaboration}}: {{The Case}} of the {{Social Sciences}} and {{Humanities}}},
  shorttitle = {E-{{Infrastructures}} for {{Research Collaboration}}},
  booktitle = {Companion of the 2017 {{ACM Conference}} on {{Computer Supported Cooperative Work}} and {{Social Computing}}},
  author = {Korn, Matthias and Schorch, Mar{\'e}n and Pipek, Volkmar and Bietz, Matthew and {\O}sterlund, Carsten and Procter, Rob and Ribes, David and Williams, Robin},
  year = {2017},
  month = feb,
  series = {{{CSCW}} '17 {{Companion}}},
  pages = {415--420},
  publisher = {{Association for Computing Machinery}},
  address = {{New York, NY, USA}},
  doi = {10.1145/3022198.3022655},
  url = {https://doi.org/10.1145/3022198.3022655},
  urldate = {2021-05-01},
  abstract = {Collaborative research practices are a highly interesting domain for CSCW. So far, CSCW has mainly focused on computation- and/or data-intensive research endeavors. Here, resources are typically pooled via common e-infrastructures for data access and processing, a set-up requiring additional layers of coordination. Such a focus largely foregrounds the sciences and other fields that rely on highly structured (or structure-able) data and the routinized processes of analysis. In contrast, in this one-day workshop we discuss the conditions and challenges characteristic of research collaboration in the qualitative social sciences and humanities (SSH). In particular, we examine the sociotechnical infrastructures that enable and support research practices that--in comparison with the collaborative paradigm of the natural sciences--tend to be less structured, compartmentalized, and routinized, but more fluid, flexible, and open-ended. The workshop seeks to collect empirical insights and design experiences, preparing the grounds for a comprehensive understanding of the role of e-infrastructures for collaborative research practices in SSH.},
  isbn = {978-1-4503-4688-7},
  keywords = {cyberinfrastructure,e-infrastructure,e-research,humanities,infrastructuring,qualitative research,social sciences},
  file = {/Users/jonny/Dropbox/papers/zotero/K/KornM/korn_2017_e-infrastructures_for_research_collaboration.pdf}
}

