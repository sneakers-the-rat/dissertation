@article{pirroDHTbasedSemanticOverlay2012,
  title = {A {{DHT-based}} Semantic Overlay Network for Service Discovery},
  author = {Pirr{\`o}, Giuseppe and Talia, Domenico and Trunfio, Paolo},
  year = {2012},
  month = apr,
  journal = {Future Generation Computer Systems},
  volume = {28},
