@article{liuOptimalFeaturesAuditory2019,
  title = {Optimal Features for Auditory Categorization},
  author = {Liu, Shi Tong and {Montes-Lourido}, Pilar and Wang, Xiaoqin and Sadagopan, Srivatsun},
  year = {2019},
