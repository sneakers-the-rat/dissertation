@misc{bartleyBuildingOpenRepresentation2022,
  title = {Building an {{Open Representation}} for {{Biological Protocols}}},
  author = {Bartley, Bryan and Beal, Jacob and Rogers, Miles and Bryce, Daniel and Goldman, Robert P. and Keller, Benjamin and Lee, Peter and Biggers, Vanessa and Nowak, Joshua and Weston, Mark},
  year = {2022},
