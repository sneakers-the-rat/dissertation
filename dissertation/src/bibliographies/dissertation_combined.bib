@article{oviedoConnectivityMotifsInhibitory2017,
  title = {Connectivity Motifs of Inhibitory Neurons in the Mouse {{Auditory Cortex}}},
  author = {Oviedo, Hysell V.},
  year = {2017},
  month = dec,
  journal = {Scientific Reports},
  volume = {7},
