@article{ribesArtifactsThatOrganize2013,
  title = {Artifacts That Organize: {{Delegation}} in the Distributed Organization},
  shorttitle = {Artifacts That Organize},
  author = {Ribes, David and Jackson, Steven and Geiger, Stuart and Burton, Matthew and Finholt, Thomas},
  year = {2013},
  month = jan,
  journal = {Information and Organization},
  volume = {23},
