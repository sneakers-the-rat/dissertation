@incollection{pachecoDigitalScienceCyberinfrastructure2018,
  title = {Digital {{Science}}: {{Cyberinfrastructure}}, e-{{Science}} and {{Citizen Science}}},
  shorttitle = {Digital {{Science}}},
  booktitle = {Knowledge {{Management}} in {{Digital Change}}: {{New Findings}} and {{Practical Cases}}},
  author = {Pacheco, Roberto C. S. and R. Nascimento, Everton and Weber, Rosina O.},
